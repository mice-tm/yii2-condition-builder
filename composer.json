{
  "name": "mice-tm/yii2-condition-builder",
  "type": "yii2-extension",
  "description": "Condition constructor for yii2",
  "keywords": ["yii2", "extension"],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Iryna Mychkova",
      "email": "imychkova@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "micetm\\conditions\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "2.0.15.1",
    "yii2tech/embedded": "~1.0.3",
    "kartik-v/yii2-widgets": "~3.4.0",
    "trntv/yii2-datetime-widget": "~1.1.0",
    "oomphinc/composer-installers-extender": "^1.1.2",
    "mice-tm/conditions-base": "^1.0.2"
  },
  "config": {
    "process-timeout": 1800
  }
}
